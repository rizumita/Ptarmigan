info description = My Project

projectId = MyProject

const Ver = v1
locale = ja

collection users {
  document User {
    firstName: string%name.firstName
    lastName: string%name.lastName

    collection notes {
      document Note {
        title: string%name.title
        tags: string%random.word[20]
        ref: ref@/categories/Category
        array: { name: string%random.word }[10]
        dict: {
          name: string%random.word
          date: timestamp%date.recent
        }

        fake = 20
      }
    }

    generate 20
    generate {
      "rizumita" : { "id": 0, "name": "Ryoichi Izumita"}
    }
  }
}
