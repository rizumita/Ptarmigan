info project = My Project

const Ver = v1
locale = ja

type ID: int@unique
type Name: string@name.firstName
type Title: string@name.title

type User: {
  id: ID
  name: Name
}

type Note: {
  id: ID
  title: Title
}

collection users {
  document User {
    collection note {
      document Note
    }
  }

  fake 100
  generate {
    "rizumita" : { "id": 0, "name": "Ryoichi Izumita"}
  }
}
