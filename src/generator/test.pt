info description = My Project

projectId = MyProject

const myself = rizumita
locale = ja

type User = {
  firstName: string%{{name.firstName}}
  field: {
    name: string%{{random.word}}
  }
}

collection users {
  id: $myself
  document User {
    collection notes {
      id: {{random.uuid}}
      document Note {
        tags: string%{{random.word}}[20]
      }
    }

    generate 100
    generate [
      { "firstName" : "Ryoichi", "lastName" : "Izumita" }
    ]
  }
}
